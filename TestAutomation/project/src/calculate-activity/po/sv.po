#: functions.py:119
msgid "abs(x), return absolute value of x, which means -x for x < 0"
msgstr "abs(x), returnera absolutbelopp för x, vilket betyder -x för x < 0"

#: functions.py:124
msgid "acos(x) only defined for x E [-1,1]"
msgstr "acos(x) endast definierad för x E [-1,1]"

# TODO: should be -1 <= x <= 1
#: functions.py:128
msgid ""
"acos(x), return the arc cosine of x. This is the angle for which the cosine "
"is x. Defined for -1 <= x < 1"
msgstr ""
"acos(x) returnerar arkus cosinus av x. Alltså den vinkel för vilken "
"cos(vinkel) = x. Definierad för -1 <= x <= 1"

#: functions.py:135
msgid ""
"acosh(x), return the arc hyperbolic cosine of x. This is the value y for "
"which the hyperbolic cosine equals x."
msgstr ""
"acosh(x), returnera arcus hyperbolisk cosinus av x. Detta är värdet y för "
"vilket hyperbolisk cosinus är lika med x."

#: functions.py:142
msgid ""
"And(x, y), logical and. Returns True if x and y are True,else returns False"
msgstr ""
"And(x, y), logiskt och. Returnerar True om x och y är true, annars "
"returneras False"

#: functions.py:151
msgid "add(x, y), return x + y"
msgstr "add(x, y), returnera x + y"

#: functions.py:156
msgid "asin(x) only defined for x E [-1,1]"
msgstr "asin(x) endast definierad för x E [-1,1]"

#: functions.py:159
msgid ""
"asin(x), return the arc sine of x. This is the angle for which the sine is "
"x. Defined for -1 <= x <= 1"
msgstr ""
"asin(x) returnerar arcus sinus av x. Alltså den vinkel för vilken sinus = x. "
"Definierad för -1 <= x <= 1"

#: functions.py:166
msgid ""
"asinh(x), return the arc hyperbolic sine of x. This is the value y for which "
"the hyperbolic sine equals x."
msgstr ""
"asinh(x), returnera arcus hyperbolisk sinus av x. Detta är värdet y för "
"vilket hyperbolisk sinus är lika med x."

#: functions.py:173
msgid ""
"atan(x), return the arc tangent of x. This is the angle for which the "
"tangent is x. Defined for all x"
msgstr ""
"atan(x) returnerar arktangens av x. Alltså den vinkel för vilken tan(vinkel) "
"= x. Alla värden på x är giltiga"

#: functions.py:180
msgid ""
"atanh(x), return the arc hyperbolic tangent of x. This is the value y for "
"which the hyperbolic tangent equals x."
msgstr ""
"atanh(x), returner arkus hyperbolisk tangens av x. Detta är värdet y för "
"vilket hyperbolisk tangens är lika med x."

#: functions.py:191
msgid "Number does not look binary."
msgstr "Tal ser inte ut att vara binärt."

#: functions.py:199
msgid ""
"b10bin(x), interpret a number written in base 10 as binary, e.g.: "
"b10bin(10111) = 23,"
msgstr ""
"b10bin(x), tolka ett tal skrivet i basen 10 som binärt, t.ex.: b10bin(10111) "
"= 23,"

#: functions.py:205
msgid "ceil(x), return the smallest integer larger than x."
msgstr "tak(x), returnera det minsta heltal som är större än x."

#: functions.py:211
msgid ""
"cos(x), return the cosine of x. This is the x-coordinate on the unit circle "
"at the angle x"
msgstr ""
"cos(x), returnerar cosinus av x. Alltså x-koordinaten på enhetscirkeln för "
"vinkeln x"

#: functions.py:218
msgid ""
"cosh(x), return the hyperbolic cosine of x.Given by (exp(x) + exp(-x)) / 2"
msgstr ""
"cosh(x), returnera hyperbolisk cosinus av x. Givet av (exp(x) + exp(-x)) / 2"

#: functions.py:224 functions.py:319
msgid "Can not divide by zero"
msgstr "Kan inte dividera med noll"

#: functions.py:247
msgid "Invalid argument"
msgstr "Ogiltigt argument"

#: functions.py:250
msgid ""
"gcd(a, b), determine the greatest common denominator of a and b. For "
"example, the biggest factor that is shared by the numbers 15 and 18 is 3."
msgstr ""
"sgd(a, b), avgör den största gemensamma delaren av a och b. Till exempel är "
"3 den största faktorn som delar 15 och 18."

#: functions.py:257
msgid "exp(x), return the natural exponent of x. Given by e^x"
msgstr "exp(x) returnerar den naturliga exponenten av x. Ges av e^x"

#: functions.py:262
msgid "Factorial(x) is only defined for integers x>=0"
msgstr "Fakultet(x) är endast definierat för heltal x>=0"

#: functions.py:265
msgid "Factorial only defined for integers"
msgstr "Fakultet är endast definierad för heltal"

#: functions.py:278
msgid ""
"factorial(n), return the factorial of n. Given by n * (n - 1) * (n - 2) * ..."
msgstr ""
"fakultet(n), returnerar fakulteten av n. Ges av n * (n - 1) * (n - 2) * ..."

#: functions.py:314
msgid "floor(x), return the largest integer smaller than x."
msgstr "golv(x), returnera det största heltal som är mindre än x."

#: functions.py:321
msgid "inv(x), return the inverse of x, which is 1 / x"
msgstr "inv(x), returnera inversen av x, vilken är 1 / x"

#: functions.py:356
msgid "Argument must be int"
msgstr "Argument måste vara heltal"

#: functions.py:358
msgid "Prime numbers is defined for natural numbers"
msgstr "Primtal är definierade för naturliga tal"

#: functions.py:368 functions.py:378
msgid "Logarithm(x) only defined for x > 0"
msgstr "Logarithm(x) endast definierad för x > 0"

#: functions.py:370
msgid ""
"ln(x), return the natural logarithm of x. This is the value for which the "
"exponent exp() equals x. Defined for x >= 0."
msgstr ""
"ln(x) returnerar den naturliga logaritmen av x. Det är det värde för vilket "
"exp(värde) = x. Är endast definierad för positiva x."

#: functions.py:380
msgid ""
"log10(x), return the base 10 logarithm of x. This is the value y for which "
"10^y equals x. Defined for x >= 0."
msgstr ""
"log10(x), returnera bas 10-logaritmen av x. Detta är värdet y för vilket 10^"
"y är lika med x. Definierad för x >= 0."

#: functions.py:388
msgid "Can only calculate x modulo <integer>"
msgstr "Kan endast beräkna x mod <heltal>"

#: functions.py:390
msgid ""
"mod(x, y), return the modulus of x with respect to y.This is the remainder "
"after dividing x by y."
msgstr ""
"mod(x, y), returnera modulo för x med avseende på y. Detta är resten efter "
"att dela x med y."

#: functions.py:399
msgid "mul(x, y), return x * y"
msgstr "mul(x, y), returnera x * y"

#: functions.py:404
msgid "negate(x), return -x"
msgstr "negate(x), returnera -x"

#: functions.py:410
#, fuzzy
msgid ""
"Or(x, y), logical or. Returns True if x or y is True, else returns False"
msgstr ""
"or(x, y), logiskt eller. Returnerar sant om någon av x eller y är sanna. Om "
"båda är falska returneras falskt"

#: functions.py:427
msgid "pow(x, y), return x to the power y (x**y)"
msgstr "power(x, y), returnera x upphöjt till y (x**y)"

#: functions.py:433
msgid "rand_float(), return a random floating point number between 0.0 and 1.0"
msgstr "rand_float(), returnera ett slumpmässigt flyttal mellan 0.0 och 1.0"

#: functions.py:439
msgid ""
"rand_int([<maxval>]), return a random integer between 0 and <maxval>. "
"<maxval> is an optional argument and is set to 65535 by default."
msgstr ""
"rand_int([<maxmal>]), returnera ett slumpmässigt heltal mellan 0 och <maxval>"
". <maxval> är ett valfritt argument och sätts till 65535 som standard."

#: functions.py:445
msgid "round(x), return the integer nearest to x."
msgstr "round(x), returnera heltalet närmast x."

#: functions.py:452 functions.py:461
msgid "Bitwise operations only apply to integers"
msgstr "Bitvisa operationer kan endas tillämpas på heltal"

#: functions.py:454
msgid "shift_left(x, y), shift x by y bits to the left (multiply by 2 per bit)"
msgstr ""
"shift_left(x, y), skifta x y bitar åt vänster (multiplicera med 2 per bit)"

#: functions.py:463
msgid "shift_right(x, y), shift x by y bits to the right (divide by 2 per bit)"
msgstr "shift_right(x, y), skifta x y bitar åt höger (dividera med 2 per bit)"

#: functions.py:469
msgid ""
"sin(x), return the sine of x. This is the y-coordinate on the unit circle at "
"the angle x"
msgstr ""
"sin(x) returnerar sinus av x. Detta är y-koordinaten på enhetscirkeln vid "
"vinkel x"

#: functions.py:476
msgid ""
"sinh(x), return the hyperbolic sine of x. Given by (exp(x) - exp(-x)) / 2"
msgstr ""
"sinh(x) returnerar sinus hyperbolikus av x. Ges av formeln (exp(x) - exp(-x))"
" / 2"

#: functions.py:485
msgid "sinc(x), return the sinc of x. This is given by sin(x) / x."
msgstr "sinc(x), returnera sinc-funktionen av x. Detta ges av sin(x) / x."

#: functions.py:491
msgid ""
"sqrt(x), return the square root of x. This is the value for which the square "
"equals x. Defined for x >= 0."
msgstr ""
"sqrt(x) returnerar kvadratorten av x. Detta är värdet för vilken svaret i "
"kvadrat är likamed x."

#: functions.py:497
msgid "square(x), return x * x"
msgstr "square(x), returnera x * x"

#: functions.py:505
msgid "sub(x, y), return x - y"
msgstr "sub(x, y), returnera x - y"

#: functions.py:511
msgid ""
"tan(x), return the tangent of x. This is the slope of the line from the "
"origin of the unit circle to the point on the unit circle defined by the "
"angle x. Given by sin(x) / cos(x)"
msgstr ""
"tan(x) returnerar tangens av x. Definierar som lutningen på kurvan som går "
"från origo på enhetscirkeln och genom den punkt som bestäms av vinkel x. Ges "
"av sin(x)/cos(x)"

#: functions.py:519
msgid "tanh(x), return the hyperbolic tangent of x. Given by sinh(x) / cosh(x)"
msgstr "tanh(x) returnerar tangens hyperbolikus av x. Ges av sinh(x) / cosh(x)"

#: functions.py:525
msgid ""
"xor(x, y), logical xor. Returns True if either x is True (and y is False) or "
"y is True (and x is False), else returns False"
msgstr ""
"xor(x, y) logiskt exklusivt eller. Fungerar som det vi i vanligt tal "
"betraktar som eller, dvs returnerar sant om antingen x eller y är sant. "
"Annars returnerar falskt"

