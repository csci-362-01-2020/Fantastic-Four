#: functions.py:119
msgid "abs(x), return absolute value of x, which means -x for x < 0"
msgstr "abs(x), trả về chính xác giá trị x, nghĩa là -x nếu x < 0"

#: functions.py:124
msgid "acos(x) only defined for x E [-1,1]"
msgstr "acos(x) chỉ định nghĩa cho x E [-1,1]"

#: functions.py:128
msgid ""
"acos(x), return the arc cosine of x. This is the angle for which the cosine "
"is x. Defined for -1 <= x < 1"
msgstr ""
"acos(x), trả lại arc cosin của x. Đây là góc mà có cosin là x. Xác định cho -"
"1 ≤ x < 1"

#: functions.py:135
msgid ""
"acosh(x), return the arc hyperbolic cosine of x. This is the value y for "
"which the hyperbolic cosine equals x."
msgstr ""
"acosh(x), trả về arc cosin hypebon của x. Đây là giá trị y mà có cosin "
"hypebon bằng x"

#: functions.py:142
msgid ""
"And(x, y), logical and. Returns True if x and y are True,else returns False"
msgstr ""
"And(x,y), logic và. Trả về giá trị Đúng nếu x và y đều Đúng, ngược lại trả "
"về Sai."

#: functions.py:151
msgid "add(x, y), return x + y"
msgstr "add(x,y), trả về giá trị x + y"

#: functions.py:156
msgid "asin(x) only defined for x E [-1,1]"
msgstr "asin(x) chỉ định nghĩa cho x E [-1,1]"

#: functions.py:159
msgid ""
"asin(x), return the arc sine of x. This is the angle for which the sine is "
"x. Defined for -1 <= x <= 1"
msgstr ""
"asin(x), trả lại arc sin của x. Đây là góc có sin là x. Xác định cho phạm vi "
"-1 ≤ x ≤ 1"

#: functions.py:166
msgid ""
"asinh(x), return the arc hyperbolic sine of x. This is the value y for which "
"the hyperbolic sine equals x."
msgstr ""
"asinh(x), trả về arc sin hypebon của x. Đây là giá trị y mà sin hypebon bằng "
"x."

#: functions.py:173
msgid ""
"atan(x), return the arc tangent of x. This is the angle for which the "
"tangent is x. Defined for all x"
msgstr ""
"atan(x), trả lại arc tang của x. Đây là góc cho đó tang là x. Xác định cho "
"toàn bộ x"

#: functions.py:180
msgid ""
"atanh(x), return the arc hyperbolic tangent of x. This is the value y for "
"which the hyperbolic tangent equals x."
msgstr ""
"atanh(x), arc tang hypebon của x. Đây là giá trị y có tang hypebon bằng x"

#: functions.py:191
msgid "Number does not look binary."
msgstr "Số không thuộc hệ nhị phân"

#: functions.py:199
msgid ""
"b10bin(x), interpret a number written in base 10 as binary, e.g.: "
"b10bin(10111) = 23,"
msgstr ""
"b10bin(x), chuyển một số từ hệ nhị phân sang hệ thập phân, ví dụ : "
"b10bin(10111) = 23"

#: functions.py:205
msgid "ceil(x), return the smallest integer larger than x."
msgstr "ceil(x), trả lại số nguyên nhỏ nhất lớn hơn x"

#: functions.py:211
msgid ""
"cos(x), return the cosine of x. This is the x-coordinate on the unit circle "
"at the angle x"
msgstr ""
"cos(x), trả lại cosin của x. Đây là toạ độ x trên hình tròn đơn vị ở góc x"

#: functions.py:218
msgid ""
"cosh(x), return the hyperbolic cosine of x.Given by (exp(x) + exp(-x)) / 2"
msgstr "cosh(x), cosin hypebon của x. Được tính theo (exp(x) + exp(-x))/2"

#: functions.py:224 functions.py:319
msgid "Can not divide by zero"
msgstr "Không thể chia cho 0"

#: functions.py:247
msgid "Invalid argument"
msgstr "Tham số không hợp lệ"

#: functions.py:250
msgid ""
"gcd(a, b), determine the greatest common denominator of a and b. For "
"example, the biggest factor that is shared by the numbers 15 and 18 is 3."
msgstr ""
"gcd(a,b), ước chung lớn nhất của a và b. Ví dụ thừa số lớn nhất được chia "
"hết bởi 15 và 18 là 3"

#: functions.py:257
msgid "exp(x), return the natural exponent of x. Given by e^x"
msgstr "exp(x), trả lại số mũ tự nhiên của x. Đưa ra bởi e^x"

#: functions.py:262
msgid "Factorial(x) is only defined for integers x>=0"
msgstr "Factorial(x) chỉ định nghĩa cho số nguyên x>=0"

#: functions.py:265
msgid "Factorial only defined for integers"
msgstr "Factorial chỉ định nghĩa cho số nguyên"

#: functions.py:278
#, fuzzy
msgid ""
"factorial(n), return the factorial of n. Given by n * (n - 1) * (n - 2) * ..."
msgstr "fac(x), trả lại thừa số của x. Đưa ra bởi x * (x - 1) * (x - 2) * ..."

#: functions.py:314
msgid "floor(x), return the largest integer smaller than x."
msgstr "floor(x), trả lại số nguyên lớn nhất nhỏ hơn x"

#: functions.py:321
msgid "inv(x), return the inverse of x, which is 1 / x"
msgstr "inv(x), nghịch đảo của x, là 1/x"

#: functions.py:356
msgid "Argument must be int"
msgstr "Tham số phải là số nguyên (int)"

#: functions.py:358
msgid "Prime numbers is defined for natural numbers"
msgstr "Số nguyên tố được định nghĩa cho số tự nhiên"

#: functions.py:368 functions.py:378
msgid "Logarithm(x) only defined for x > 0"
msgstr "Logarithm(x) chỉ xác định với x > 0"

#: functions.py:370
msgid ""
"ln(x), return the natural logarithm of x. This is the value for which the "
"exponent exp() equals x. Defined for x >= 0."
msgstr ""
"ln(x), trả lại lôgarit tự nhiên của x. Đây là giá trị cho đó số mũ exp() "
"bằng x. Xác định cho phạm vi x ≥ 0."

#: functions.py:380
msgid ""
"log10(x), return the base 10 logarithm of x. This is the value y for which "
"10^y equals x. Defined for x >= 0."
msgstr ""
"log10(x), trả về logarit cơ số 10 của x. Đây là giá trị y mà 10 mũ y bằng x "
"(10^y = x). Với x >=0."

#: functions.py:388
msgid "Can only calculate x modulo <integer>"
msgstr "Chỉ có thể tính số dư của x chia cho <integer>"

#: functions.py:390
msgid ""
"mod(x, y), return the modulus of x with respect to y.This is the remainder "
"after dividing x by y."
msgstr ""
"mod(x,y), trả lại số dư của x chia y. Đây là số dư sau khi chia x cho y."

#: functions.py:399
msgid "mul(x, y), return x * y"
msgstr "mul(x,y), trả lại giá trị x nhân y"

#: functions.py:404
msgid "negate(x), return -x"
msgstr "negate(x), trả về -x"

#: functions.py:410
#, fuzzy
msgid ""
"Or(x, y), logical or. Returns True if x or y is True, else returns False"
msgstr ""
"or(x, y), « hoặc » lô gic. Là Đúng nếu x Đúng và/hay y Đúng. Nếu cả x Sai và "
"y Sai thì trả lại Sai."

#: functions.py:427
msgid "pow(x, y), return x to the power y (x**y)"
msgstr "pow(x,y), trả lại giá trị x luỹ thừa y (hay x mũ y)"

#: functions.py:433
msgid "rand_float(), return a random floating point number between 0.0 and 1.0"
msgstr "rand_float(), trả lại giá trị ngẫu nhiên giữa 0 và 1.0"

#: functions.py:439
msgid ""
"rand_int([<maxval>]), return a random integer between 0 and <maxval>. "
"<maxval> is an optional argument and is set to 65535 by default."
msgstr ""
"rand_int([<maxval>]), trả lại giá trị ngẫu nhiên giữa 0 và <maxval>. <maxval>"
" là một tham số tùy chọn và được đặt là 65535 theo mặc định."

#: functions.py:445
msgid "round(x), return the integer nearest to x."
msgstr "round(x), trả lại giá trị nguyên gần nhất với x"

#: functions.py:452 functions.py:461
msgid "Bitwise operations only apply to integers"
msgstr "Phép toán thao tác bit chỉ áp dụng cho các số nguyên"

#: functions.py:454
msgid "shift_left(x, y), shift x by y bits to the left (multiply by 2 per bit)"
msgstr "shift_left(x, y), dich x sang bên trái y bit (nhân 2 cho mỗi bit dịch)"

#: functions.py:463
#, fuzzy
msgid "shift_right(x, y), shift x by y bits to the right (divide by 2 per bit)"
msgstr "shift_left(x, y), dich x sang bên trái y bit (nhân 2 cho mỗi bit dịch)"

#: functions.py:469
msgid ""
"sin(x), return the sine of x. This is the y-coordinate on the unit circle at "
"the angle x"
msgstr ""
"sin(x), trả lại sin của x. Đây là toạ độ y trên hình tròn đơn vị ở góc x"

#: functions.py:476
msgid ""
"sinh(x), return the hyperbolic sine of x. Given by (exp(x) - exp(-x)) / 2"
msgstr "sinh(x), trả lại sin hyperbol của x. Đưa ra bởi (exp(x) - exp(-x)) / 2"

#: functions.py:485
msgid "sinc(x), return the sinc of x. This is given by sin(x) / x."
msgstr ""

#: functions.py:491
msgid ""
"sqrt(x), return the square root of x. This is the value for which the square "
"equals x. Defined for x >= 0."
msgstr ""
"sqrt(x), trả lại căn bậc hai của x. Đây là giá trị cho đó bình phương bằng "
"x. Xác định cho phạm vi x ≥ 0."

#: functions.py:497
msgid "square(x), return x * x"
msgstr "square(x), trả lại giá trị x * x"

#: functions.py:505
#, fuzzy
msgid "sub(x, y), return x - y"
msgstr "mul(x,y), trả lại giá trị x * y"

#: functions.py:511
msgid ""
"tan(x), return the tangent of x. This is the slope of the line from the "
"origin of the unit circle to the point on the unit circle defined by the "
"angle x. Given by sin(x) / cos(x)"
msgstr ""
"tan(x), trả lại tang của x. Đây là dốc của đường từ gốc của hình tròn đơn vị "
"đến điểm trên hình tròn đơn vị xác định theo góc x. Đưa ra bởi sin(x) / "
"cos(x)"

#: functions.py:519
#, fuzzy
msgid "tanh(x), return the hyperbolic tangent of x. Given by sinh(x) / cosh(x)"
msgstr "sinh(x), trả lại tang hyperbol của x. Đưa ra bởi sinh(x) / cosh(x)"

#: functions.py:525
msgid ""
"xor(x, y), logical xor. Returns True if either x is True (and y is False) or "
"y is True (and x is False), else returns False"
msgstr ""
"xor(x, y), xor hợp lý. Trả lại Đúng nếu X là đúng (và y là Sai) hoặc y là "
"Đúng (và x là Sai), không thì trả lại Sai"

